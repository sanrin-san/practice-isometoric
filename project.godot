config_version=5

[application]

config/name="practice-isometoric"
run/main_scene="res://Levels/level_1.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Global="*res://Utils/global.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"amina": "res://Dialogic/character/amina.dch"
}
directories/dtl_directory={
"test": "res://Dialogic/timeline/test.dtl"
}
layout/style_list=["res://Dialogic/New_File.tres"]
layout/default_style="res://Dialogic/New_File.tres"
glossary/default_case_sensitive=true
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={
"player_name": "default"
}
animations/join_default="Fade In"
animations/leave_default="Fade Out"
translation/enabled=true
translation/original_locale="ja"
translation/file_mode=1
translation/translation_folder="res://Dialogic/translate"
translation/save_mode=0
translation/add_separator=false
translation/intern/save_mode=0
translation/intern/file_mode=1
translation/intern/translation_folder="res://Dialogic/translate"
translation/id_counter=21
translation/locales=[]

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/gloot/plugin.cfg")

